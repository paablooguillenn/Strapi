{
    "kind": "collectionType",
    "collectionName": "phases",
    "info": {
        "singularName": "phase",
        "pluralName": "phases",
        "displayName": "svdPhase"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string",
            "required": true
        },
        "sortOrder": {
            "type": "integer",
            "required": true,
            "unique": true
        },
        "description": {
            "type": "richtext"
        },
        "project": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::project.project",
            "inversedBy": "phases"
        },
        "subphases": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::subphase.subphase",
            "mappedBy": "phase"
        }
    }
}
