{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "svdProject"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "projectStatus": {
      "type": "enumeration",
      "default": "ACTIVE",
      "enum": [
        "ACTIVE",
        "ARCHIVED"
      ]
    },
    "phases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::phase.phase",
      "mappedBy": "project"
    },
    "editions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::edition.edition",
      "mappedBy": "project"
    },
    "metrics": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::metric.metric",
      "mappedBy": "project"
    }
  }
}
